.modal-confirm.modal.fade#modalBlogs
	.modal-dialog
		.modal-content
			.modal-header
				button.close(data-dismiss='modal') x
				h4 Your data has been saved
			.modal-body
				input#blog-btn-submit(type='submit',value='Create your blog')
			.modal-footer
				button(data-dismiss="modal").cancel.btn.btn-default Cancel
				button(data-dismiss="modal").submit.btn.btn-primary Ok